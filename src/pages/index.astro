---
import { config } from "@/config";
import MainLayout from "@/layouts/MainLayout.astro";
import Contact from "@/components/sections/Contact.astro";
import Screens from "@/components/sections/Screens.astro";
import Faqs from "@/components/sections/Faqs.astro";
import Features from "@/components/sections/Features.astro";
import Hero from "@/components/sections/Hero.astro";
import Plans from "@/components/sections/Plans.astro";
import CallToAction from "@/components/sections/CallToAction.astro";
---

<MainLayout title={config.title} description={config.description}>
  <Hero />
  <Screens />
  <Features />
  <CallToAction />
  <Faqs />
  <Plans />
  <Contact />
</MainLayout>
